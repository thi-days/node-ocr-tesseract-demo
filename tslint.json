{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-console": true,
    "no-consecutive-blank-lines": true,
    "arrow-return-shorthand": true,
    "class-name": true,
    "curly": true,
    "eofline": true,
    "import-spacing": true,
    "no-any": true,
    "semicolon": true,
    "quotemark": [true, "single"],
    "no-trailing-whitespace": true
  }
}